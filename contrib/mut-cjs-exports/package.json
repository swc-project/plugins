{
    "name": "@swc-contrib/mut-cjs-exports",
    "version": "14.0.1",
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "description": "[SWC plugin] mutable CJS exports",
    "author": "magic-akari <akari.ccino@gmail.com>",
    "license": "MIT",
    "keywords": [
        "swc-plugin",
        "swc",
        "jest",
        "cjs",
        "commonjs"
    ],
    "scripts": {
        "build": "RUSTFLAGS='--cfg swc_ast_unknown' cargo build --release --target wasm32-unknown-unknown && cp ../../target/wasm32-unknown-unknown/release/swc_mut_cjs_exports.wasm .",
        "build:debug": "RUSTFLAGS='--cfg swc_ast_unknown' cargo build --target wasm32-unknown-unknown && cp ../../target/wasm32-unknown-unknown/debug/swc_mut_cjs_exports.wasm ./swc_mut_cjs_exports_debug.wasm",
        "test": "pnpm run build:debug && jest",
        "prepack": "pnpm run build"
    },
    "packageManager": "pnpm@8.15.5",
    "main": "swc_mut_cjs_exports.wasm",
    "files": [
        "src",
        "swc_mut_cjs_exports.wasm"
    ],
    "homepage": "https://github.com/swc-project/plugins/tree/main/contrib/mut-cjs-exports",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/swc-project/plugins.git"
    },
    "bugs": {
        "url": "https://github.com/swc-project/plugins/issues"
    },
    "devDependencies": {
        "@swc/jest": "^0.2.37",
        "@types/jest": "^29.5.11",
        "jest": "^29.7.0"
    },
    "peerDependencies": {
        "@swc/core": "^1.10.0",
        "@swc/jest": "^0.2.37"
    }
}